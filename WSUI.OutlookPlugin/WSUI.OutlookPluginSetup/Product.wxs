      <ComponentRef Id="ELASTICSEASRCHINSTALLFOLDERINGESTCOMMON" />
      <ComponentRef Id="ELASTICSEASRCHINSTALLFOLDERLANGEXPRESSION" />
      <ComponentRef Id="ELASTICSEASRCHINSTALLFOLDERLANGGROOVY" />
      <ComponentRef Id="ELASTICSEASRCHINSTALLFOLDERLANGMUSTACHE" />
