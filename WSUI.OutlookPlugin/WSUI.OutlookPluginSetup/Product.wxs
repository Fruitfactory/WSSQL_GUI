      <UpgradeVersion Minimum="0.0.0.0" IncludeMinimum="yes" OnlyDetect="no" Maximum="10.0.0.0" IncludeMaximum="yes" Property="PREVIOUSFOUND" />
    </Upgrade>
    <Media Id="1" Cabinet="OutlookFinder.cab" EmbedCab="yes" />
