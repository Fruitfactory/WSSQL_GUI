    <CustomAction Id="CheckVersion" BinaryKey="managedAction" DllEntry="CheckVersion" Execute="immediate" Return="check" />
    <CustomAction Id="InstallRules" BinaryKey="managedAction" DllEntry="InstallRules" Execute="immediate" Return="check"></CustomAction>
    <InstallExecuteSequence>
      <RemoveExistingProducts After="InstallInitialize" />
      <Custom Action="RegisterDll" After="SelfRegModules">NOT Installed</Custom>
      <Custom Action="ResetRegistry" After="RegisterDll">NOT Installed</Custom>
      <Custom Action="UnregisterDll" After="SelfUnregModules">Installed</Custom>
      <Custom Action="CheckVersion" Before="InstallInitialize" />
      <Custom Action="Deactivate" Before="InstallValidate">Installed OR REMOVE="ALL"</Custom>
      <!--<Custom Action="StopServiceApp" After="UninstallRules">Installed</Custom>-->
      <Custom Action="DeleteFiles" After="InstallFinalize">Installed OR REMOVE="ALL"</Custom>
      <Custom Action="SetInstallElasticSearchProperties" Before="InstallFinalize">NOT Installed</Custom>
      <!--<Custom Action="InstallElasticSearch" After="SetInstallElasticSearchProperties">NOT Installed</Custom>-->
      <Custom Action="StartServiceApp" After="InstallFinalize">NOT Installed</Custom>
      <Custom Action="InstallApp" After="StartServiceApp">NOT Installed</Custom>
      <Custom Action="ClosePrompt" Before="InstallValidate">NOT Installed OR Installed OR REMOVE="ALL"</Custom>
      <Custom Action="SuccesMessage" OnExit="success">NOT Installed</Custom>
      <Custom Action="CancelMessage" OnExit="cancel">NOT Installed</Custom>
      <Custom Action="ErrorMessage" OnExit="error">NOT Installed</Custom>
    </InstallExecuteSequence>
    <!-- Properties for all current versions of the .NET Framework are available here: http://wix.sourceforge.net/manual-wix3/wixnetfxextension.htm -->
    <PropertyRef Id="NETFRAMEWORK45" />
    <Condition Message="This application requires .NET Framework 4.5. Please install the .NET Framework then run this installer again."><![CDATA[Installed OR NETFRAMEWORK45]]></Condition>
